<?xml version='1.0' encoding='utf-8'?>
<widget id="com.chancesrmis.ertdashboard" version="1.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Insight BCP Mobile</name>
    <description>Insight BCP Mobile</description>
    <author email="santiagogerman@hotmail.com" href="https://chancesrmis.com">
        Ing. Santiago Gastanadui Lujan
    </author>
    <content src="index.html" />
    <preference name="android-build-tool" value="ant" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-push" spec="1.4.2" source="pgb" />
    <plugin name="cordova-plugin-actionsheet" version="2.2.2" />
    <plugin name="cordova-plugin-compat" version="1.0.0" />
    <plugin name="cordova-plugin-console" version="1.0.3" />
    <plugin name="cordova-plugin-dialogs" version="1.2.1" />
    <plugin name="cordova-plugin-file" version="4.2.0" />
    <plugin name="cordova-plugin-geolocation" version="2.2.0" />
    <plugin name="cordova-plugin-media" version="2.3.0" />
    <plugin name="cordova-plugin-network-information" version="1.2.1" />
    <plugin name="org.flybuy.cordova.background-location-services" version="1.0.0" />
    <plugin name="org.frd49.cordova.exitapp" version="1.0.0" />
    <plugin name="uk.co.workingedge.phonegap.plugin.launchnavigator" version="3.0.4" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
